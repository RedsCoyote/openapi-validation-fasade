{
    "name": "redscoyote/openapi-validation-fasade",
    "description": "Validates data against provide openapi spec",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "mmal/openapi-validator": "^0.9",
        "phpunit/phpunit": "^9.2",
        "psr/http-message": "^1.0",
        "symfony/yaml": "^5.2"
    },
    "require-dev": {
        "nyholm/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
